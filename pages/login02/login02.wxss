/*登陆*/
.login_main {
width: 100% !important;
    margin-left: auto;
    margin-right: auto;
    z-index: 999!important;
    background-color: #FFFFFF;
    height: 80%!important;
    position: absolute;
    padding-top: 20%;
}

.login_main_head {
	width: 80px;
	height: 80px;
	margin: 20px auto;

	border-radius: 50%;
	padding: 3px;
	overflow: hidden;
}

.login_main_img {
	width: 100%;
	height: 100%;
	background: rgba(255, 255, 255, .8);
	border-radius: 50%;
	position: relative;
}

.login_main_img .login_camera {
	width: 42px;
	height: 30px;
	background: url(../img/camera.png) no-repeat center;
	background-size: 100%;
	display: inline-block;
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
}
.login_name{margin-left: 20%;}
.qh{position: absolute; left: 0px; top:10px; text-align: center;border-right-width: 1px; border-right-style: solid; border-right-color: #D6D6D6; width: 17%;}
.login_main_box {
	width: 100%;
	overflow: hidden;
}

.login_main_list {
	width: 90%;
	overflow: hidden;
	border-bottom: 1px solid #e8e8e8;
	padding: 8px 0;
	margin: 0 auto;
}

.login_main_list span {
	color: #ccc;
	line-height: 42px;
	font-size: 14px;
}

.login_main_list input {
	font-family: "微软雅黑";
}

.login_main_list input::-webkit-input-placeholder {
	color: #ccc !important;
}

.login_submit,
.register_submit,
.passWord_submit,
.release_submit_button,
.pop_submit_button,.recharge_submit_button,.join_submit_button {
	width: 80%;
	margin: 40px auto 0 auto;
	background: #5fcaa6;
	color: #fff;
	height: 40px;
	line-height: 40px;
	border-radius: 20px;
	text-align: center;
}

.login_bottom_way,
.login_bottom_main {
	width: 90%;
	margin: 10px auto 0 auto;
	overflow: hidden;
}

.login_bottom_way span {
	color: #5fcaa6;
	display: inline-block;
}

.login_bottom_main p {
	margin: 0;
	text-align: center;
	position: relative;
	color: #ccc;
	font-size: 12px;
}

.login_bottom_main p:after {
	position: absolute;
	right: 0;
	top: 0;
	bottom: 0;
	margin: auto;
	width: 30%;
	height: 1px;
	content: '';
	background: #E6E6E6;
}

.login_bottom_main p:before {
	position: absolute;
	left: 0;
	top: 0;
	bottom: 0;
	margin: auto;
	width: 30%;
	height: 1px;
	content: '';
	background: #E6E6E6;
}

.login_main_way {
	width: 100%;
	text-align: center;
	padding: 15px 0;
}

.weixin_weixin {
	width: 30px;
	height: 25px;
	display: inline-block;
	background: url(../images/weixin.png) no-repeat center;
	background-size: 100% 100%;
}

.login_main_way span {
	color: #ccc;
	font-size: 12px;
	display: block;
}
.information_name_main input, .user_password_list input, .login_main_list input {
    width: 100% !important;
    margin-bottom: 0 !important;
    font-size: 16px !important;
    border: none !important;
    padding: 0 15px 0 0 !important;
    color: #666666 !important;
    display: inline-block;
}
.login_main_img img{ width: 100%;}
